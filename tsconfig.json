{
    "compilerOptions": {
        "outDir": "./dump",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "jsxFactory": "React.createElement",
        "declaration": true,
        "declarationDir": "./dist",
        "alwaysStrict": true,
        "listEmittedFiles": true
    },
    "include": [
        "./lib/**/*"
    ]
}

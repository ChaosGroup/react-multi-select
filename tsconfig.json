{
	"compilerOptions": {
		"outDir": "./dump",
		"lib": ["es2015", "es2016", "es2017", "DOM"],
		"sourceMap": true,
		"noImplicitAny": true,
		"declaration": true,
		"declarationDir": "./dist",
		"alwaysStrict": true,
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"isolatedModules": false,
		"jsx": "react"
	},
	"include": [
		"./lib/**/*"
	]
}
